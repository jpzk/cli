{
  "extends": [
    "tslint:latest",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "rules": {
    "prettier": true,
    "no-implicit-dependencies": false,
    "no-reference": false,
    "no-this-assignment": false,
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "interface-name": [true, "never-prefix"]
  }
}
