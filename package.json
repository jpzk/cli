{
  "name": "dcl-cli",
  "version": "0.1.3",
  "description": "CLI tool for parcel management.",
  "main": "./dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "npm run clean && node_modules/typescript/bin/tsc && npm run copy:template",
    "clean": "rimraf dist && rimraf tmp",
    "copy:template": "copyfiles -u 1 template/* template/**/* dist/template",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "tsc": "tsc",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decentraland/cli.git"
  },
  "keywords": [
    "decentraland",
    "cli"
  ],
  "author": "Decentraland",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/decentraland/cli/issues"
  },
  "homepage": "https://github.com/decentraland/cli#readme",
  "bin": {
    "dcl": "./bin/index.js"
  },
  "dependencies": {
    "@types/fs-extra": "^5.0.0",
    "budo": "^10.0.4",
    "chalk": "^2.3.0",
    "decentraland-commons": "git+https://github.com/decentraland/commons.git#release",
    "fs-extra": "^5.0.0",
    "ipfs": "^0.27.5",
    "ipfs-api": "^17.2.6",
    "progress": "^2.0.0",
    "root-check": "^1.0.0",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/chalk": "^2.2.0",
    "@types/mocha": "^2.2.45",
    "@types/node": "^8.5.2",
    "@types/progress": "^2.0.1",
    "chai": "^4.1.2",
    "copyfiles": "^1.2.0",
    "mocha": "^4.1.0",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.2",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.6.2"
  }
}
